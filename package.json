{
  "name": "gulp-armarapify",
  "version": "1.0.0",
  "description": "The plugin for Gulp which allows to rapify (binarize) ArmA *.hpp/*.ext files",
  "main": "dist/main.js",
  "scripts": {
    "install:typings": "./node_modules/.bin/typings install",
    "install:dev": "npm install && npm run install:typings",
    "test": "./node_modules/.bin/gulp test:run",
    "cover": "./node_modules/.bin/gulp cover:run",
    "assemble": "./node_modules/.bin/gulp assemble",
    "watch": "./node_modules/.bin/gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winseros/gulp-armarapify-plugin.git"
  },
  "keywords": [
    "gulpplugin",
    "arma",
    "arma2",
    "arma3",
    "gulp",
    "rapify",
    "rap",
    "binarize"
  ],
  "author": "winse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/winseros/gulp-armarapify-plugin/issues"
  },
  "homepage": "https://github.com/winseros/gulp-armarapify-plugin#readme",
  "engines": {
    "node": ">=6.9.0"
  },
  "dependencies": {
    "gulp-util": "^3.0.7",
    "vinyl": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-preset-env": "^1.1.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-jasmine": "^2.4.2",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-tslint": "^7.0.1",
    "gulp-typescript": "^3.1.3",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.2",
    "jasmine-console-reporter": "^1.2.7",
    "karma": "^1.3.0",
    "merge-stream": "^1.0.1",
    "remap-istanbul": "^0.8.4",
    "tslint": "^4.4.2",
    "typescript": "^2.0.10",
    "typings": "^2.1.0"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "!**/test/"
  ]
}
